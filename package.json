    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "formidable": "^1.2.1",
    "hbs": "~4.0.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.7",
