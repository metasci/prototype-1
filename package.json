    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "^4.18.2",
    "formidable": "^1.2.2",
    "hbs": "^4.1.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^1.7.0",
    "nodemailer": "^4.7.0",
